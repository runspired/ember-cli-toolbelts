{
  "name": "ember-cli-toolbelts",
  "version": "0.3.3",
  "description": "Specialized blueprints for maintaining and iterating faster",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc addon/",
    "lint-all": "eslint -c .eslintrc app/ addon/ tests/",
    "lint-staged": "git diff --diff-filter=ACMRTUXB --cached --name-only | grep '*.js' | grep -v 'node_modules' | grep -v 'tests' | xargs eslint -c .eslintrc"
  },
  "repository": "https://github.com/runspired/ember-cli-toolbelts.git",
  "engines": {
    "node": ">= 4.1.1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.1",
    "ember-cli": "^2.2.0-beta.6",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-changelog": "^0.3.4",
    "ember-cli-content-security-policy": "^0.4.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^1.1.0",
    "ember-cli-release": "^0.2.8",
    "ember-cli-uglify": "^1.2.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "0.0.8"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "jsonfile": "^2.2.3",
    "lodash": "^4.17.19",
    "object-assign": "^4.0.1",
    "rsvp": "^3.1.0",
    "valid-url": "^1.0.9"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
