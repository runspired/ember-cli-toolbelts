{
  "name": "ember-cli-toolbelts",
  "version": "0.1.0",
  "description": "The default blueprint for ember-cli addons.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc addon/",
    "lint-all": "eslint -c .eslintrc app/ addon/ tests/",
    "lint-staged": "git diff --diff-filter=ACMRTUXB --cached --name-only | grep '*.js' | grep -v 'node_modules' | grep -v 'tests' | xargs eslint -c .eslintrc"
  },
  "repository": "https://github.com/runspired/ember-cli-toolbelts.git",
  "engines": {
    "node": ">= 4.1.1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-cli": "1.13.12",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.1.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "0.2.4",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.0.4",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "1.13.15",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-try": "~0.0.8"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "ember-cli-babel": "^5.1.5",
    "jsonfile": "^2.2.3",
    "lodash": "^3.10.1",
    "object-assign": "^4.0.1",
    "pleasant-progress": "^1.1.0",
    "rsvp": "^3.1.0",
    "valid-url": "^1.0.9"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
